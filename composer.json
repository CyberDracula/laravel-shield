{
    "name": "vinkla/shield",
    "description": "A HTTP basic auth middleware for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "http", "basic", "auth", "middleware", "shield"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com",
            "homepage": "https://vinkla.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.3.* || 5.4.*",
        "symfony/http-kernel": "^3.3"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Shield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Shield\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Shield\\ShieldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
